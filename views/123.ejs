


<div class="body-content outer-top-xs">
	<div class="container">
		<div class="row ">
			<div class="shopping-cart">
				<div class="shopping-cart-table ">
					<% if(messages.success) { %>
						<div class="w3-panel w3-green">
							<h3>Success!</h3>
							<p><%= messages.success %></p>
						  </div>
						
	
						<% } %>
	<div class="table-responsive">
		<table class="table">
			<thead>
				<tr>
					<th class="cart-qty item">My profile</th>
					<th class="cart-description item">Manage and protect your account</th>

				</tr>
			</thead><!-- /thead -->
			<tfoot>
				<tr>
					<td colspan="7">
						<div class="shopping-cart-btn">
							<span class="">
								<a href="/" class="btn btn-upper btn-primary outer-left-xs">Continue Shopping</a>
								<a href="/shopping-cart" class="btn btn-upper btn-primary pull-right outer-right-xs">Update shopping cart</a>
							</span>
						</div><!-- /.shopping-cart-btn -->
					</td>
				</tr>
			</tfoot>
			<tbody>

				
		
				<tr>
					<td class="cart-product-sub-total"><span class="cart-sub-total-price"> Username: </span></td>
					<td class="cart-product-sub-total"><span class="cart-sub-total-price"> </span></td>
				</tr>
				<tr>
					<td class="cart-product-sub-total"><span class="cart-sub-total-price"> Name: </span></td>
					<td class="cart-product-sub-total"><span class="cart-sub-total-price"> </span></td>
				</tr>
				<tr>
					<td class="cart-product-sub-total"><span class="cart-sub-total-price"> Birth date: </span></td>
					<td class="cart-product-sub-total"><span class="cart-sub-total-price"> </span></td>
				</tr>
				<tr>
					<td class="cart-product-sub-total"><span class="cart-sub-total-price"> Gender: </span></td>
					<td class="cart-product-sub-total"><span class="cart-sub-total-price"> </span></td>
				</tr>
				<tr>
					<td class="cart-product-sub-total"><span class="cart-sub-total-price"> Phone Number: </span></td>
					<td class="cart-product-sub-total"><span class="cart-sub-total-price"> </span></td>
				</tr>
				<tr>
					<td class="cart-product-sub-total"><span class="cart-sub-total-price"> Address: </span></td>
					<td class="cart-product-sub-total"><span class="cart-sub-total-price"> </span></td>
				</tr>
			
			</tbody><!-- /tbody -->
		</table><!-- /table -->
	</div>



