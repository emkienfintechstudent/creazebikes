          <script>
            // Example data, replace this with your actual data source
            const subcategories = {
                1: [
                    { id: 1, name: 'Subcategory 1.1' },
                    { id: 2, name: 'Subcategory 1.2' }
                ],
                2: [
                    { id: 3, name: 'Subcategory 2.1' },
                    { id: 4, name: 'Subcategory 2.2' }
                ]
            };
    
            $(document).ready(function() {
                $('#categorySelect').change(function() {
                    const categoryId = $(this).val();
                    const subcategorySelect = $('#subcategorySelect');
    
                    // Clear existing options
                    subcategorySelect.empty();
    
                    // Populate subcategories based on the selected category
                    if (subcategories[categoryId]) {
                        subcategories[categoryId].forEach(subcategory => {
                            subcategorySelect.append(new Option(subcategory.name, subcategory.id));
                        });
                    }
                });
            });
        </script>
